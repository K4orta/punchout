{
  "manifest_version": 2,

  "name": "Punchout",
  "permissions": [
    "http://0.0.0.0:8081/",
    "http://localhost:8081/",
    "http://0.0.0.0:8081/*",
    "http://localhost:8081/*",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://0value.com/"],
      "js": ["jquery.min.js", "app.js"]
    }
  ],
  "description": "This extension helps hourly peons clock in and out",
  "version": "1.0"
}